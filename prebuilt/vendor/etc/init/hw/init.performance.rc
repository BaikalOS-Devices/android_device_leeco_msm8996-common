    write /sys/devices/system/cpu/cpu3/sched_static_cpu_pwr_cost 500
    write /sys/devices/system/cpu/cpu3/sched_static_cluster_pwr_cost 1000

    write /sys/devices/system/cpu/cpu0/core_ctl/min_cpus ${sys.baikal.min_cpus:-2}
    write /sys/devices/system/cpu/cpu2/core_ctl/min_cpus ${sys.baikal.min_cpus:-2}


    write /sys/devices/system/cpu/cpu0/core_ctl/busy_down_thres 35
    write /sys/devices/system/cpu/cpu0/core_ctl/busy_up_thres 45
    write /sys/devices/system/cpu/cpu2/core_ctl/busy_down_thres 55
